<view><view name="referrer" content="never" class="_meta"></view><image class="background" src="{{BGURL}}" mode="aspectFill"></image><view style="{{(windowTop)}}"></view><scroll-view style="{{(windowHeight)}}" scroll-y="true" show-scrollbar="false"><view class="main"><text class="title">正在学习</text><view class="BookContainer1"><view class="BookContainer2"><view><view class="ChooseBook"><image class="book1" src="{{book.image}}"></image><view class="book2 smallFont">更换书籍<image class="img" src="/static/image/Set/add.png" data-event-opts="{{[['tap',[['toLibrary',['$event']]]]]}}" bindtap="__e"></image></view></view><text class="mainColor">{{book.name}}</text></view><view class="progress-box"><progress activeColor="#F09137" percent="{{book.study/book.sumWords*100}}" stroke-width="10" active="{{true}}"></progress><view class="wordsText"><text>{{"已学习 "+book.study}}</text><text>{{"总词数 "+book.sumWords}}</text></view></view></view></view><text class="title">我的数据</text><view class="dataContainer1"><view class="dataContainer2"><text>概览</text><view class="dataContainer3"><study-data vue-id="0548b87a-1" title="今日学习" number="{{con_learn}}" imageSrc="/static/image/Set/today.png" unit="词" bind:__l="__l"></study-data><study-data vue-id="0548b87a-2" title="累计学习" number="{{sum_learn}}" imageSrc="/static/image/Set/sum.png" unit="词" bind:__l="__l"></study-data></view><view class="dataContainer3"><study-data vue-id="0548b87a-3" title="最大连续签到" number="{{con_day}}" imageSrc="/static/image/Set/time.png" unit="天" bind:__l="__l"></study-data><study-data vue-id="0548b87a-4" title="累计签到" number="{{sum_day}}" imageSrc="/static/image/Set/clock_in.png" unit="天" bind:__l="__l"></study-data></view><text>日历</text></view><uni-calendar class="calender" vue-id="0548b87a-5" selected="{{date}}" bind:__l="__l"></uni-calendar></view></view></scroll-view></view>